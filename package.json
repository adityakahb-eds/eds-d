{
  "name": "eds-c",
  "private": true,
  "version": "1.0.0",
  "description": "Starter project for Adobe Helix",
  "scripts": {
    "build": "gulp build",
    "build:qa": "cross-env WMK_ENV=qa gulp build",
    "build:sit": "cross-env WMK_ENV=sit gulp build",
    "build:uat": "cross-env WMK_ENV=uat gulp build",
    "build:stage": "cross-env WMK_ENV=stage gulp build",
    "build:main": "cross-env WMK_ENV=main gulp build",
    "gulper": "gulp",
    "lint:css": "stylelint src/**/*.scss --allow-empty-input",
    "lint:js": "eslint .",
    "lint": "npm run lint:js && npm run lint:css",
    "start": "npm-run-all --parallel gulper up",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "up": "aem up"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adobe/aem-boilerplate.git"
  },
  "author": "Adobe",
  "license": "Apache License 2.0",
  "bugs": {
    "url": "https://github.com/adobe/aem-boilerplate/issues"
  },
  "homepage": "https://github.com/adobe/aem-boilerplate#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "glob": "^11.0.3",
    "gulp": "^5.0.1",
    "gulp-clean": "^0.4.0",
    "gulp-eslint-new": "^2.4.0",
    "gulp-rename": "^2.1.0",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^6.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "html-minifier-terser": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.91.0",
    "stylelint": "^16.23.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.0"
  }
}
